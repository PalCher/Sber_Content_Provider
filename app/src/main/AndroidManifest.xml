<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nexp.pavel.notepad_contentprovider">

    <uses-permission android:name="com.nexp.pavel.ass_hm_notepad_room.PERMISSION"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    <activity android:name=".NewNote"
        android:label="@string/newNote">
    </activity>
    <activity android:name=".UpdateNote"
        android:label="@string/updateNote">
    </activity>
    </application>

</manifest>